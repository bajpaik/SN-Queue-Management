        <template><![CDATA[<section ng-class="{'location-content': c.data.enableOnlineCheckIn, 'location-content-landscape': !c.data.enableOnlineCheckIn}">
  <h4 class="h4 padding-left">
    {{data.msgs.aboutLocation}}
  </h4>
  <div class="schedule-summary padding-right">
    <div ng-if="c.location.image" class="image-container padding-left">
      <img ng-src="{{c.location.image}}" alt="${Location image}"/>
    </div>
    <div class="schedule-info padding-left padding-bottom">
      <p class="location-description">
        {{c.location.description}}
      </p>
      <p>
        <strong ng-if="c.hasSchedule">
          {{data.msgs.loungeHours}}
        </strong>
      </p>
      <table class="table table-condensed table-striped table-responsive" aria-label="${Schedule for the week}" ng-if="c.hasSchedule">
        <tr class="sr-only">
          <th>${Day of the week}</th>
          <th>${Hours of operation}</th>
        </tr>
        <tr ng-repeat="(day, scheduleEntry) in c.scheduleSpan">
          <td class="day-tag" aria-label="{{scheduleEntry.fullDay}}">{{scheduleEntry.abbreviated}}</td>
          <td class="schedule-tag">
            <div ng-if="scheduleEntry.list.length > 0" ng-repeat="timeEntry in scheduleEntry.list">
              {{timeEntry.start}} ${to} {{timeEntry.end}}
            </div>
            <div ng-if="scheduleEntry.list.length === 0">
              {{data.msgs.closed}}
            </div>
          </td>
        </tr>
      </table>
      <div>
        <p ng-if="c.hasSchedule && c.data.lastCheckIn > 0">
          * {{data.noteScheduleMsg}}
        </p>
      </div>
    </div>
  </div>
</section>]]></template>
    </sp_ng_template>
</record_update>
