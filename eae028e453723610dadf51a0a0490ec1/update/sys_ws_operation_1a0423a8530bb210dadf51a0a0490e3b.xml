        <name>Fetch Users currently being Serviced</name>
        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var pathParm = request.pathParams;
    var queryParm = request.queryParams;
    if (!pathParm.locationId || !queryParm.state) {
        gs.error('Location not found while fetching users currently in the queue being serviced');
        return;
    }

    var api = new GQI_WalkupHelper().fetchOnlineServiced(pathParm.locationId, queryParm.state);

    var apiResponse = JSON.parse(api.Response);
    if (apiResponse != null)
        return apiResponse.result;
    else
        return apiResponse;

})(request, response);]]></operation_script>
