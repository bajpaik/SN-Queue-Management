        <name>Fetch Walkup Locations</name>
        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var grWLQ = new GlideAggregate('wu_location_queue');
    grWLQ.addEncodedQuery('x_80404_gqi_queu_0_walkup_campus!=NULL^active=true');
    grWLQ.query();
    var locations = [];
    while (grWLQ.next()) {
        locations.push({
            sys_id: grWLQ.x_80404_gqi_queu_0_walkup_campus.sys_id,
            name: grWLQ.x_80404_gqi_queu_0_walkup_campus.name.toString()
        });
    }
    response.setBody(locations);
})(request, response);]]></operation_script>
