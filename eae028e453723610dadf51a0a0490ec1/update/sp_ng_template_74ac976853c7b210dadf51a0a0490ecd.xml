        <template><![CDATA[<section class="hours-of-op" ng-class="{'location-content': c.data.enableOnlineCheckIn, 'location-content-landscape': !c.data.enableOnlineCheckIn}">
  <h4 class="h4 padding-left" ng-if="c.locationLoaded && !c.isClosed(c.data.scheduleWidgets[0].data.schedule) && c.data.hasSchedule" aria-label="${Current and upcoming hours of operation}">
    {{data.msgs.hoursMessage}}
  </h4>
  <h4 class="h4 padding-left" ng-if="c.locationLoaded && c.isClosed(c.data.scheduleWidgets[0].data.schedule) && c.data.hasSchedule" aria-label="${Upcoming hours of operation}">
    {{data.msgs.closedHoursMessage}}
    
  </h4>

  <div class="schedule-info padding-left padding-bottom" ng-if="c.data.hasSchedule">
    <div class="schedule-entry" ng-if="!c.isClosed(c.data.scheduleWidgets[0].data.schedule)">
      <div class="day-tag">
        {{c.data.scheduleObj.today.day}}
      </div>
      <div class="schedule-tag">
        <div ng-repeat="todayHour in c.data.scheduleObj.today.hours">
          {{todayHour}}
        </div>
      </div>
    </div>
    
    <div class="schedule-entry">
      <div class="day-tag">
        {{c.data.scheduleObj.nextAvail.day}}
      </div>
      <div class="schedule-tag">
        <div ng-repeat="nextHour in c.data.scheduleObj.nextAvail.hours">
          {{nextHour}}
        </div>
      </div>
    </div>

    <div class="disclaimer" >
      {{c.data.scheduleWidgets[0].data.lastCheckInMsg}}
    </div>
  </div>

</section>]]></template>
    </sp_ng_template>
</record_update>
