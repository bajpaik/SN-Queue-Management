        <name>Get Walkup Queues</name>
        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

 var campus = request.queryParams.campus_location_id;
    var grWLQ = new GlideRecord('wu_location_queue');
    grWLQ.addEncodedQuery('x_80404_gqi_queu_0_walkup_campus=' + campus);
    grWLQ.orderBy('order');
    grWLQ.query();
    var queues = [];
    while (grWLQ.next()) {
        queues.push({
            sys_id: grWLQ.sys_id.toString(),
            name: grWLQ.name.toString()
        });
    }
    return queues;

})(request, response);]]></operation_script>
